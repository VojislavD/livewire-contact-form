{
    "name": "vojislavd/livewire-contact-form",
    "description": "Send email from contact form with Livewire",
    "keywords": [
        "vojislavd",
        "laravel",
        "livewire-contact-form"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vojislav Dragicevic",
            "email": "vojislavdragicevic17@gmail.com"
        }
    ],
    "require": {
        "livewire/livewire": "^2.7"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "VojislavD\\LivewireContactForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VojislavD\\LivewireContactForm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VojislavD\\LivewireContactForm\\LivewireContactFormServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
